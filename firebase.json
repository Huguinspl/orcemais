{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "storage": {
    "rules": "storage.rules"
  },
  "hosting": {
    "public": "build/web",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log"
      ]
    }
  ],
  "flutter": {
    "platforms": {
      "android": {
        "default": {
          "projectId": "gestorfy-app",
          "appId": "1:388082198829:android:8988c83f6892518976cba4",
          "fileOutput": "android/app/google-services.json"
        }
      },
      "dart": {
        "lib/firebase_options.dart": {
          "projectId": "gestorfy-app",
          "configurations": {
            "android": "1:388082198829:android:8988c83f6892518976cba4",
            "ios": "1:388082198829:ios:90880ae09d4c3f9c76cba4",
            "macos": "1:388082198829:ios:90880ae09d4c3f9c76cba4",
            "web": "1:388082198829:web:080b0e26e2b1a3fd76cba4",
            "windows": "1:388082198829:web:b08380495ada9f1a76cba4"
          }
        }
      }
    }
  }
}